        "lint:fix": "eslint --fix ./src/* ./tests/*"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^4.10.1",
        "@types/node": "^20.12.1",
        "@vitest/coverage-v8": "^3.0.7",
        "eslint": "^9.22.0",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "vitest": "^3.0.7"
    }
