{
    "name": "@milesight/json2mq",
    "version": "1.0.0",
    "description": "Convert JSON data to media query string",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
